<div
  class="tab-pane fade show active"
  id="detail-client"
  role="tabpanel"
  aria-labelledby="profile-tab"
>
  <div class="card shadow mb-4">
    <div class="card-body">
      <form action="/admin/clients" method="POST" enctype="multipart/form-data">
        <div class="form-group">
          <label for="name">Nama Client</label>
          <input
            type="text"
            class="form-control"
            name="nama"
            placeholder="Masukkan nama client"
            value="<%= clients.nama %>"
            readonly
            required
          />
        </div>
        <div class="form-group">
          <label for="name">Logo</label>
          <br />
          <img
            src="https://weba-admin.herokuapp.com/<%= clients.imageUrl %> "
            alt="imageUrl"
            height="100px"
          />
        </div>
        <a href="/admin/clients" class="btn btn-warning">Back</a>
      </form>
    </div>
  </div>
</div>
